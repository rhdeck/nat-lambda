{
  "name": "webhooks",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.ts --max-warnings 0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cp base.yml serverless.yml && serverless-lambda-builder serverless && serverless-yaml",
    "build:assets": "rm -rf assets ; local-assets start && (mkdir assets ; touch assets/default)",
    "deploy": "yarn lint && yarn build:assets && serverless-stage deploy --force",
    "deployfnc": "yarn lint && serverless-stage deployskip -f",
    "logs": "serverless-stage logs -t -f ",
    "remove": "serverless-stage remove",
    "onwatch": "yarn build && yarn deploy",
    "watch": "yarn rebuild && yarn livelink watch",
    "rewatch": "domino await \"webhooks:start\" && yarn watch",
    "rebuild": "yarn build && yarn deploy --force && noti -m \"webhooks force deployed\" && domino push \"webhooks:rebuild\"",
    "clean": "(cd ../.. && yarn clearcache) && yarn ; npx fix-local-dependencies && domino push \"webhooks:clean\"",
    "restart": "yarn clean && domino push \"webhooks:start\" && yarn livelink start",
    "setup": "serverless-stage setup"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@raydeck/controller-manager": "^1.13.0",
    "@raydeck/event-manager": "^1.2.1",
    "@raydeck/livelink": "^1.5.2",
    "@raydeck/local-assets": "^1.3.0",
    "@raydeck/registry-manager": "^1.3.0",
    "@raydeck/s3-manager": "^2.3.1",
    "@raydeck/serverless-console": "^1.0.0",
    "@raydeck/serverless-setup": "^3.1.2",
    "@raydeck/session-manager": "^1.4.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-fetch": "^3.1.4",
    "epsagon": "^1.81.4",
    "raw-loader": "^4.0.2",
    "serverless-api-gateway-throttling": "^1.0.1",
    "serverless-plugin-info-json": "^1.0.2",
    "serverless-plugin-warmup": "^4.9.0",
    "serverless-vpc-plugin": "^1.0.4",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@raydeck/domino": "^1.1.1",
    "@raydeck/graphdoc": "^2.4.0",
    "@raydeck/graphql-schema-typescript": "^1.3.2",
    "@raydeck/merge-graphql": "^1.1.0",
    "@raydeck/serverless-base": "^1.0.4",
    "@raydeck/serverless-builder": "^2.0.2",
    "@raydeck/serverless-lambda-builder": "^2.2.2",
    "@raydeck/serverless-stage": "^3.1.1",
    "@raydeck/serverless-yaml": "^1.4.1",
    "@types/aws-lambda": "^8.10.59",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "serverless": "^1.78.1",
    "serverless-prune-plugin": "^1.4.3",
    "serverless-webpack": "^5.3.3",
    "source-map-loader": "^1.0.1",
    "ts-loader": "^8.0.2",
    "typescript": "^4.2.4",
    "webpack": "^4.44.1",
    "webpack-aws-externals": "^2.0.1"
  }
